<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler"
	xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson"
	xmlns:f="/lib/form" xmlns:i="jelly:fmt">
	<table>
	<j:if test="${it.hasChildren()}">
		<tr><td><strong>Suites: ${it.getChildren().size()}</strong></td></tr>
		<tr><th>Name</th><th>Pages</th><th>Right</th><th>Wrong</th><th>Ignored</th><th>Exceptions</th></tr>
	    <j:forEach var="r" items="${it.getChildren()}">
			<tr><td><a href="${r.name}">${r.name}</a></td><td>${r.totalCount}</td><td>${r.passCount}</td><td>${r.failOnlyCount}</td><td>${r.ignoredCount}</td><td>${r.exceptionCount}</td></tr>
	    </j:forEach>
	</j:if>
	</table>
</j:jelly>
